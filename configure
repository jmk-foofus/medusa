show_build_status()
{
  if test "$1" = "true" ; then
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: $2 Enabled" >&5
printf "%s\n" "$as_me: $2 Enabled" >&6;}
  else
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: $2 ** Disabled **" >&5
printf "%s\n" "$as_me: $2 ** Disabled **" >&6;}
  fi
}

show_build_status "${enable_module_afp}"        "    AFP            "
